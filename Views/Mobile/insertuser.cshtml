@model external.Models.TBLUser
@{
    ViewBag.Title = "insertuser";
}



<h2>Sign UP</h2>

@using (Html.BeginForm("insertuser", "Mobile", FormMethod.Post))
{
    <div class="container">
        <div class="form-group">
            <label>User Name</label>
            @Html.TextBoxFor(tb => tb.u_name)
        </div>
    </div>
    <div class="container">
        <div class="form-group">
            <label>User pass</label>
            @Html.TextBoxFor(tb => tb.u_psw)
        </div>
    </div>
    <div class="container">
        <button type="submit">SignUp</button>
    </div>


    @Html.ActionLink("LOGIN", "Loginuser")
}